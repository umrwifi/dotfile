{
  "coc.preferences.extensionUpdateCheck": "daily",
  "suggest.noselect": true,
  "suggest.enablePreview": false,
  "codeLens.enable": true,
  "diagnostic.refreshOnInsertMode": false,
  "coc.preferences.formatOnSaveFiletypes": [
    "json"
  ],
  "imselect.defaultInput": "com.apple.keylayout.ABC",
  "explorer.width": 30,
  "explorer.previewAction.onHover": true,
  "explorer.file.autoReveal": true,
  "explorer.keyMappings.global": {
    "i": false,
    "gk": "expandablePrev",
    "gj": "expandableNext",
    "*": "toggleSelection",
    "<tab>": "actionMenu",
    "h": "collapse",
    "l": [
      "expandable?",
      [
        "expand"
      ],
      "open"
    ],
    "J": [
      "toggleSelection",
      "nodeNext"
    ],
    "K": [
      "toggleSelection",
      "nodePrev"
    ],
    "gl": "expandRecursive",
    "gh": "collapseRecursive",
    "<LeftMouse>": [
      "expandable?",
      [
        "expanded?",
        "collapse",
        "expand"
      ],
      "open"
    ],
    "o": [
      "expanded?",
      "collapse",
      "expand"
    ],
    "<cr>": false,
    "e": "open",
    "s": "open:split",
    "S": "open:split:plain",
    "E": "open:vsplit",
    "t": "open:tab",
    "<bs>": "gotoParent",
    "gp": "pasteFile",
    "y": "copyFilepath",
    "Y": "copyFilename",
    "c": "copyFile",
    "x": "cutFile",
    "p": "paste",
    "d": "delete",
    "D": "deleteForever",
    "n": "addFile",
    "N": "addDirectory",
    "r": "rename",
    ".": "toggleHidden",
    "R": "refresh",
    "?": "help",
    "q": false,
    "<esc>": "esc",
    "gx": "systemExecute",
    "gd": "listDrive",
    "f": "search",
    "F": "searchRecursive",
    "gf": "gotoSource:file",
    "gb": "gotoSource:buffer",
    "[[": "sourcePrev",
    "]]": "sourceNext",
    "[d": "diagnosticPrev",
    "]d": "diagnosticNext",
    "[c": "gitPrev",
    "]c": "gitNext",
    "<<": "gitStage",
    ">>": "gitUnstage"
  },
  "html.autoClosingTags": true,
  "html.format.preserveNewLines": true
}
