{
	"title":"cmd to enter/esc",
  "rules": [
{
  "description": "set lcmd to esc if press alone",
  "manipulators": [
    {
      "from": {
        "key_code": "left_gui",
        "modifiers": {
          "optional": [
            "any"
          ]
        }
      },
      "to": [
        {
          "key_code": "left_gui"
        }
      ],
      "to_if_alone": [
        {
          "key_code": "escape"
        }
      ],
      "type": "basic"
    }
  ]
},
{
  "description": "set rcmd to enter if press alone",
  "manipulators": [
    {
      "from": {
        "key_code": "right_gui",
        "modifiers": {
          "optional": [
            "any"
          ]
        }
      },
      "to": [
        {
          "key_code": "right_gui"
        }
      ],
      "to_if_alone": [
        {
          "key_code": "return_or_enter"
        }
      ],
      "type": "basic"
    }
  ]
}
  ]
}
