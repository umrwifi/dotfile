{
  "title": " switch command tab",
  "rules": [
    {
      "description": "Change command+j to arrow keys",
      "manipulators": [
        {
          "from": {
            "key_code": "right_shift"
          },
          "to": [
            {
              "key_code": "tab"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Change command+k to arrow keys",
      "manipulators": [
        {
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [
                "left_command"
              ],
              "optional": [
                "caps_lock",
                "command"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
